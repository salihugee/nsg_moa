AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Azure and Power BI Configuration
AZURE_TENANT_ID=your_tenant_id
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret
AZURE_REDIRECT_URI=http://localhost:8000/callback

POWERBI_WORKSPACE_ID=your_workspace_id
POWERBI_DATASET_ID=your_dataset_id
POWERBI_REPORT_ID=your_report_id
POWERBI_SCOPE=https://analysis.windows.net/powerbi/api/.default

# Weather API Configuration
WEATHER_API_KEY=your_api_key
WEATHER_API_URL=https://api.openweathermap.org/data/2.5
WEATHER_CACHE_DURATION=1800

# Maps Configuration
MAPS_PROVIDER=google
GOOGLE_MAPS_API_KEY=your_google_maps_key
GEOCODING_API_ENDPOINT=https://maps.googleapis.com/maps/api/geocode/json

# SMS Gateway Configuration
SMS_API_URL=https://api.sms.gateway.com
SMS_API_KEY=your_sms_api_key
SMS_SENDER_ID=NSG-MOA
SMS_TIMEOUT=30
SMS_RETRY_ATTEMPTS=3
SMS_RETRY_DELAY=5
SMS_WEBHOOK_URL=https://your-domain.com/api/sms/webhook
SMS_STATUS_ENDPOINT=https://api.sms.gateway.com/status

# Database Configuration
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=nsg_moa
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

# Monitoring Configuration
MONITORING_LOG_CHANNEL=stack
MONITORING_EMAIL=alerts@nsgmoa.gov.ng
MONITORING_ALERT_THRESHOLD=90
METRICS_RETENTION_DAYS=30

# Queue Configuration
QUEUE_CONNECTION=database
QUEUE_WORKER_SLEEP=3
QUEUE_MAX_TRIES=3
QUEUE_TIMEOUT=60
